{
  "name": "microdigitall-website",
  "private": true,
  "version": "1.0.0",
  "description": "MicroDigitall Website with Contact Form",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
